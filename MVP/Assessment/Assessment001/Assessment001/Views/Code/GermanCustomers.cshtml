@{
    Layout = null;
    var customers = Model;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>German Customers</title>
</head>
<body>
    <div>
        <h2>German Customers</h2>
        <table>
            <tr>
                <th>Customer ID</th>
                <th>Company Name</th>
                <th>Country</th>
                <!-- Add other required columns -->
            </tr>
            @foreach (var customer in Model)
            {
                <tr>
                    <td>@customer.CustomerID</td>
                    <td>@customer.CompanyName</td>
                    <td>@customer.Country</td>
                    <!-- Add other required columns -->
                </tr>
            }
        </table>
    </div>
</body>
</html>